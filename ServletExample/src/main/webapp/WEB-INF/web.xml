<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>Hello</servlet-name>
		<servlet-class>com.revature.servlets.HelloServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Bean</servlet-name>
		<servlet-class>com.revature.servlets.BeanServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>FrontController</servlet-name>
		<servlet-class>com.revature.servlets.FrontControllerServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Hello</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Bean</servlet-name>
		<url-pattern>/beans</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>


</web-app>
